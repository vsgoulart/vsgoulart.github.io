  "scripts": {
    "build": "bnr build",
    "serve": "bnr serve",
    "clean": "bnr clean"
  },
  "betterScripts": {
    "build": {
      "command": "bnr clean && webpack --env.prod -p && gulp copy-index-to-root",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "serve": {
      "command": "webpack-dev-server",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "clean": {
      "command": "rimraf dist"
    }
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.29.0",
    "normalize.css": "^7.0.0",
    "postcss": "^6.0.8",
    "postcss-loader": "^2.0.6",
    "precss": "^2.0.0",
    "style-loader": "^0.18.2",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1"
  },
  "dependencies": {
    "better-npm-run": "^0.0.15",
    "gulp": "^3.9.1",
    "rimraf": "^2.6.1"
  }
}
